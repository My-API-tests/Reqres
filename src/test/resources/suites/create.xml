<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Create">
    <listeners>
        <listener class-name="org.qa.listener.TestListener"/>
    </listeners>

    <parameter name="fileName" value="REQ_IncorrectKeys.json"/>

    <test name="Tests creating an user providing name and job">
        <classes>
            <class name="CreateUserTest">
                <methods>
                    <include name="incorrectKeyInRequestBody"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>